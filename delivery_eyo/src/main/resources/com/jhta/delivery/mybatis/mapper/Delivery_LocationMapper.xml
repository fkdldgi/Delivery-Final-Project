<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mybatis.Delivery_LocationMapper">
	<insert id="insert" parameterType="delivery_loc">
		insert into delivery_location values(delivery_location_seq.nextval,shop_seq.currval,#{sido},#{sigungu},#{hname})
	</insert>
	<select id="selectLoc" parameterType="int" resultType="delivery_loc">
		select * from delivery_location where shop_num=#{num}
	</select>
</mapper>