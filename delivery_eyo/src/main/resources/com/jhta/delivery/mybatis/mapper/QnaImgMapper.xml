<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://www.ibatis.com/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mybatis.QnaImgMapper">
	<insert id="insert" parameterType="QnaImg">
		insert into INQUIRY_IMG values(inquiry_img_seq.nextval,#{imgPath},#{originalFilename},#{inquiryNum})
	</insert>
	<select id="imgselect" resultType="QnaImg">
		select * from INQUIRY_IMG where inquiryNum={num}
	</select>
</mapper>